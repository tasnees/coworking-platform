{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "^/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "^/_next/data/(.*)",
      "dest": "/_next/data/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/$",
      "dest": "/"
    },
    {
      "src": "^/(.*)",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "/auth/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
